{
    "manifest_version": 2,
    "name": "hackeador alto calibre",
    "version": "1.7",
    "description": "version plus pro ultra",
    "permissions": ["activeTab", "tabs"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["http://*/*"],
        "js": ["content.js"]
      }
    ]
  }
  